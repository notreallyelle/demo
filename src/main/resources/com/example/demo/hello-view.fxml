<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.text.Text?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.demo.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>


    <Text layoutX="45.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Email:" wrappingWidth="69.677734375" />
    <TextField id="Email" fx:id="email" layoutX="115.0" layoutY="28.0" />
    <Text layoutY="95.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Password:" wrappingWidth="69.677734375" />
    <TextField id="Password"  fx:id="password" layoutX="115.0" layoutY="78.0" />

    <Button text="Submit" onAction="#onButtonClick"/>

    <Label fx:id="welcomeText"/>
</VBox>